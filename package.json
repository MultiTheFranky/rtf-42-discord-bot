{
  "name": "rtf-42nd-discord-bot",
  "version": "0.0.1",
  "description": "A Discord Bot for 42nd RTF Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/MultiTheFranky/rtf-42nd-discord-bot"
  },
  "author": "Francisco Álamo García",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "precommit": "yarn lint && yarn prettier",
    "prepush": "yarn build",
    "dev": "env-cmd tsnd -r tsconfig-paths/register --respawn src/server.ts",
    "start": "env-cmd yarn lint && ts-node src/server.ts",
    "server": "env-cmd node dist/server.js",
    "build": "tsc && tsc-alias",
    "clean": "rimraf dist && rimraf log && rimraf db && rimraf node_modules && yarn",
    "lint": "eslint src --ext .ts,.tsx",
    "prettier": "prettier --write \"**/*.{ts,tsx}\"",
    "audit": "yarn-audit-fix"
  },
  "dependencies": {
    "@avionrx/pterodactyl-js": "2.1.5",
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "0.17.0",
    "@distube/soundcloud": "1.3.3",
    "@distube/spotify": "1.6.1",
    "@distube/yt-dlp": "1.1.3",
    "axios": "1.6.8",
    "cron": "3.1.6",
    "discord.js": "^14.14.1",
    "distube": "4.2.2",
    "express": "^4.18.2",
    "gamedig": "5.0.0-beta.2",
    "jsdom": "24.0.0",
    "simple-json-db": "^2.0.0",
    "sodium-native": "4.1.1",
    "turndown": "7.1.3",
    "typescript": "^5.2.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "16.2.1",
    "@types/copyfiles": "^2",
    "@types/express": "^4.17.17",
    "@types/gamedig": "5.0.1",
    "@types/jsdom": "21.1.6",
    "@types/node": "^18.15.3",
    "@types/turndown": "5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "env-cmd": "10.1.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "8.0.1",
    "nodemon": "^2.0.21",
    "prettier": "3.2.5",
    "prettier-eslint": "16.3.0",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "4.2.0"
  },
  "resolutions": {
    "@distube/ytpl": "patch:@distube/ytpl@npm:1.2.1#.yarn/patches/@distube-ytpl-npm-1.2.1-9b3ebcfa7b.patch",
    "distube": "patch:distube@npm:4.2.2#.yarn/patches/distube-npm-4.2.2-49767bfca2.patch"
  }
}
